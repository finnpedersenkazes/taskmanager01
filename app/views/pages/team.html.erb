<div class="container text-center">
  <div class="col-xs-12 col-sm-6 col-sm-offset-3">
    <h1 class="text-center">Meet the team</h1>

    <table class="table table-bordered">
      <thead>
        <tr>
          <th class="text-center">Name</th>
        </tr>
      </thead>
      <tbody>
        <% @members.each do |member| %>
          <tr>
            <td><%= member%></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
